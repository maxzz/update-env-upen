{
    "name": "upen",
    "version": "1.0.0",
    "description": "A CLI utility to update .env files with current date for _MODIFIED variables and increment build numbers for _BUILD variables",
    "author": "Sonnet 4 and Max Zakharzhevski",
    "license": "MIT",
    "keywords": [
        "cli",
        "env",
        "environment",
        "build",
        "automation",
        "typescript"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "main": "dist/main.js",
    "bin": {
        "upen": "bin/cli.js"
    },
    "files": [
        "dist/",
        "bin/",
        "README.md"
    ],
    "devDependencies": {
        "@types/node": "^20.0.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "commander": "^11.0.0"
    },
    "scripts": {
        "tsc": "tsc -w",
        "build": "tsc",
        "dev": "tsc --watch",
        "dev:tsx": "tsx src/main.ts --verbose",
        "prepublishOnly": "pnpm run build",
        "test": "node dist/main.js",
        "start": "node dist/main.js"
    }
}
